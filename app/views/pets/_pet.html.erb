<div id="<%= dom_id pet %>">
  <p>
    <strong>Name:</strong>
    <%= pet.name %>
  </p>

  <p>
    <strong>Pet type:</strong>
    <%= pet.pet_type %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= pet.description %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= pet.date %>
  </p>

  <p>
    <strong>User:</strong>
    <%= pet.user_id %>
  </p>

</div>
